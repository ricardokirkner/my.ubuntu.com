    "css-modules-require-hook": "^4.0.5",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "express-winston": "^2.0.0",
    "helmet": "^3.1.0",
    "images-require-hook": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
